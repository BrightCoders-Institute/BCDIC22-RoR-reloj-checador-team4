<%= form_with model: @company do |form|  %>
  <div>
    <%= form.label :Name %><br>
    <%= form.text_field :title, placeholder: "Current Name" %>
  </div>
  <div>
    <%= form.label :Address %><br>
    <%= form.text_field :body, placeholder: "Current Address" %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>